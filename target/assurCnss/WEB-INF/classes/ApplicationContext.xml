<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
">

    <bean id="dao" class="Models.EmployeeDao" />
    <bean id="info" class="Models.Employee" />


    <bean id="agentDao" class="Dao.AgentDao" />
    <bean id="agentInfo" class="Models.Agent" />
    <bean id="adminDao" class="Dao.AdminDao" />
    <bean id="adminInfo" class="Models.Admin" />
    <bean id="patientDao" class="Dao.PatientDao" />
    <bean id="patientInfo" class="Models.Patient" />
    <bean id="doctorDao" class="Dao.DoctorDao" />
    <bean id="doctorInfo" class="Models.Doctor" />
    <bean id="radioDao" class="Dao.RadioDao" />
    <bean id="radioInfo" class="Models.Radio" />
    <bean id="analysisDao" class="Dao.AnalysisDao" />
    <bean id="analysisInfo" class="Models.Analysis" />
    <bean id="dossierDao" class="Dao.DossierDao" />
    <bean id="dossierInfo" class="Models.Dossier" />

</beans>